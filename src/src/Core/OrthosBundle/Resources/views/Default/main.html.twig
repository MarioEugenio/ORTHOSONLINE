{% endblock %}

{% block js %}
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/jquery-1.9.1.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/jquery-ui-1.8.20.custom.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/bootstrap.js') }}"></script>
