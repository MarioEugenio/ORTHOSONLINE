<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/jquery.json-2.4.js') }}"></script>

<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/Core.js') }}"></script>

<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/loading.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/load.js') }}"></script>

<script type="text/javascript" src="{{ asset('bundles/coreorthos/default/js/menu.js') }}"></script>
<script type="text/javascript">
    $(document).ready(function() {
        $("a[rel='tooltip']").tooltip({'placement': 'bottom', 'z-index': '3000'});
    });
</script>
{% endblock %}

{% block body %}
<input type="hidden" id="clinica" value="{{clinica}}">
<input type="hidden" id="user" value="{{user}}">
<div id="view-page" ng-controller="menuCtrl">

    <div id="modalSenha" class="modal hide fade">
        <div ng-include src="srcAlterarSenha"></div>
    </div>

    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="brand" href="#">ORTHOS</a>
                <div class="nav-collapse collapse">
                    <ul class="nav">
                        <li class="active">
                            <a class="icoMenu" rel="tooltip" title="Página Inicial" href="{{ app.request.baseurl }}/orthos/main"">
                                <i class="icon-home icon-white"></i>
                            </a>
                        </li>
                        <li>
                            <a class="icoMenu" rel="tooltip" title="Ajuda" href="#">
                                <i class="icon-question-sign icon-white"></i>
                            </a>
                        </li>
                        <li class="dropdown">
                            <a  class="dropdown-toggle" data-toggle="dropdown" rel="tooltip" title="Contato" href="#">
                                <i class="icon-envelope icon-white"></i> <b class="caret"></b>
                            </a>

                            <ul class="dropdown-menu">
                                <li>
                                    <div  ng-include src="getContato()"></div>
                                </li>
                            </ul>
                        </li>

                        <li class="divider-vertical"></li>

                    </ul>

                    <ul class="nav pull-right">
                        <li class="divider-vertical"></li>

                        <li>
                            <a class="icoMenu tooltipNW"  rel="tooltip" title="Pesquisa Paciente" ng-click="linkUrl('/orthos/paciente')" original-title="Pesquisar Usuário" href="#">
                                <i class="icon-search icon-white"></i>
                            </a>
                        </li>

                        <li>
                            <a class="icoMenu tooltipNW"  rel="tooltip" title="Agenda" ng-click="linkUrl('/orthos/agenda')" original-title="Agendamento" href="#">
                                <i class="icon-calendar icon-white"></i>
                            </a>
                        </li>

                        <li class="divider-vertical"></li>

                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Menu Principal<b class="caret"></b></a>
                            {% raw %}
                            <ul class="dropdown-menu">
                                <li ng-class="getSubmenu(item)" ng-repeat="item in listMenu"><a href="#"ng-click="link(item)">{{item.no_menu}}</a>
                                    <ul ng-show="item.filhos" class="dropdown-menu">
                                        <li ng-repeat="item2 in item.filhos"><a href="#" ng-click="link(item2)">{{item2.no_menu}}</a></li>
                                    </ul>
                                </li>
                            </ul>
                            {% endraw %}
                        </li>

                        <li class="divider-vertical"></li>

                        <li class="dropdown">
                            <a class="dropdown-toggle"  rel="tooltip" title="Configuração" href="#" data-toggle="dropdown">

                                <i class="icon-user icon-white"></i>
                                <b class="caret"></b>
                            </a>
                            {% raw %}
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu">
                                <li class="dropdown">
                                    <table style="width: 200px;margin-left: 10px;">
                                        <tbody>
                                            <tr>
                                                <td><img width="40" class="img-polaroid" src="{{getImagem(user)}}"></td>
                                                <td>
                                                    <span id="j-Layouts-user-name">{{user.no_usuario}}</span><br>
                                                    <a href="#" ng-click="linkUrl('/usuario/alterar/' + user.sq_usuario)">Editar Perfil</a>
                                                </td>
                                            </tr>
                                        </tbody>
                                   </table>
                                </li>
                                <li class="divider"></li>
                                <li class="dropdown">
                                    <a original-title="Alterar Senha" ng-click="setModalSenha()" href="#">
                                        <span class="icon-edit"></span> Alterar minha senha
                                    </a>
                                </li>

                                <li class="divider"></li>
                                <li class="dropdown">
                                    <a original-title="Logoff" ng-click="logoff()" href="#">
                                        <span class="icon-remove-circle"></span> Sair do sistema
                                    </a>
                                </li>
                            </ul>
                            {% endraw %}
                        </li>
                    </ul>
                </div><!--/.nav-collapse -->
            </div>
        </div>
    </div>

    <div class="content row-fluid">
        <div ng-include src="templateMenu" class="padding-content"></div>
    </div>
</div>
{% endblock %}


