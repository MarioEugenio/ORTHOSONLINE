<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/form.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/modal.js') }}"></script>

<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/sticky.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/jquery.calendarPicker.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/jquery.printElement.js') }}"></script>

<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/angular/angular.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/angular/angular-resource.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/angular/angular-cookies.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/angular/angular-sanitize.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/angular/angular-bootstrap.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/angular/angular-bootstrap-prettify.min.js') }}"></script>
