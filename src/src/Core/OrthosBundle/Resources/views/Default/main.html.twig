    <link href="{{ asset('bundles/coreorthos/css/bootstrap-responsive.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/coreorthos/css/layout.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/coreorthos/css/base.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/coreorthos/css/jquery.calendarPicker.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/coreorthos/css/stick.min.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/coreorthos/css/jquery.dataTables.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/coreorthos/css/bootstrap-timepicker.css') }}" rel="stylesheet" type="text/css" />
