<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/bootstrap-multiselect.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/bootstrap-timepicker.js') }}"></script>

<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/jquery-maskInput.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/jquery-maskMoney.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/jquery-price-format.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/editor/jquery-te-1.3.6.min.js') }}"></script>

<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/jquery-numeric.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/jquery.dataTables.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/coreorthos/javascript/externals/jquery-cookie.js') }}"></script>
