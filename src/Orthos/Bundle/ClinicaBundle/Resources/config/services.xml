<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Orthos.BancoBusiness" class="Orthos\Bundle\ClinicaBundle\Business\BancoBusiness">
            <argument type="service" id="service_container"/>
        </service>

        <service id="Orthos.CadeiraBusiness" class="Orthos\Bundle\ClinicaBundle\Business\CadeiraBusiness">
            <argument type="service" id="service_container"/>
        </service>

        <service id="Orthos.ClinicaBusiness" class="Orthos\Bundle\ClinicaBundle\Business\ClinicaBusiness">
            <argument type="service" id="service_container"/>
        </service>

        <service id="Orthos.PacienteStatusBusiness" class="Orthos\Bundle\ClinicaBundle\Business\PacienteStatusBusiness">
            <argument type="service" id="service_container"/>
        </service>

        <service id="Orthos.ConvenioBusiness" class="Orthos\Bundle\ClinicaBundle\Business\ConvenioBusiness">
            <argument type="service" id="service_container"/>
        </service>
    </services>
</container>
