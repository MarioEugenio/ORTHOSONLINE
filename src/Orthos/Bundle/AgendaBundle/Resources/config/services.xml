<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Orthos.ConsultaBusiness" class="Orthos\Bundle\AgendaBundle\Business\ConsultaBusiness">
            <argument type="service" id="service_container"/>
        </service>

        <service id="Orthos.AgendaBusiness" class="Orthos\Bundle\AgendaBundle\Business\AgendaBusiness">
            <argument type="service" id="service_container"/>
        </service>

        <service id="Orthos.StatusBusiness" class="Orthos\Bundle\AgendaBundle\Business\StatusBusiness">
            <argument type="service" id="service_container"/>
        </service>
    </services>
</container>
